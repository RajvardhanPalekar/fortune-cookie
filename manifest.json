{
    "manifest_version": 3,
    "name": "Fortune Cookie Generator",
    "version": "1.0",
    "description": "Get random positive messages like a fortune cookie!",
    "icons": {
      "48": "icon.png",
      "128": "icon.png"
    },
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["notifications"]
  }
  